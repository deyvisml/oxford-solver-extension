{
  "manifest_version": 3,
  "name": "Oxford Solver",
  "version": "1.0.1",

  "description": "Extensión web para la resolución de la plataforma oxford",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["scripts/my-styles.css"],
      "js": ["scripts/content-script.js", "scripts/all.js"]
    }
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["images/key.png"]
    }
  ]
}
